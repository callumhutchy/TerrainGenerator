{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++.exe build active file",
            "command": "C:\\w64devkit\\bin\\g++.exe",
            "dependsOn": "_copydependenciestobuildfolder",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "-O1",
                "${file}",
                "${fileDirname}\\raylibExtras\\rlImGui.cpp",
                "${fileDirname}\\..\\submodules\\imgui\\imgui.cpp",
                "${fileDirname}\\..\\submodules\\imgui\\imgui_demo.cpp",
                "${fileDirname}\\..\\submodules\\imgui\\imgui_draw.cpp",
                "${fileDirname}\\..\\submodules\\imgui\\imgui_tables.cpp",
                "${fileDirname}\\..\\submodules\\imgui\\imgui_widgets.cpp",
                "-o",
                "${fileDirname}\\..\\builds\\${fileBasenameNoExtension}.exe",
                "-I${fileDirname}\\..\\vcpkg_installed\\x64-windows\\include",
                "-I${fileDirname}\\..\\submodules\\imgui",
                "-I${fileDirname}\\..\\submodules\\cpp-profiler",
                "-I${fileDirname}\\..\\submodules\\PerlinNoise",
                "-I${fileDirname}\\..\\src",
                "-I${fileDirname}\\..\\src\\RayLibExtras",
                "-I${fileDirname}\\..\\src\\MapGeneration",
                "-I${fileDirname}\\..\\src\\Utils",
                "-L${fileDirname}\\..\\vcpkg_installed\\x64-windows\\lib",
                "-lraylib",
                "-lgdi32",
                "-lwinmm"
            ],
            "options": {
                "cwd": "${fileDirname}\\.."
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "label": "_copydependenciestobuildfolder",
            "dependsOn": "_makebuildfolder",
            "type": "shell",
            "command": "cp ${fileDirname}\\..\\vcpkg_installed\\x64-windows\\bin\\*.dll ${fileDirname}\\..\\builds",
            "problemMatcher": [],
            "group": "none"
        },
        {
            "label": "_makebuildfolder",
            "type": "shell",
            "command": "mkdir -p ${fileDirname}\\..\\builds -Force",
            "problemMatcher": [],
            "group": "none"
        }
    ],
    "version": "2.0.0"
}